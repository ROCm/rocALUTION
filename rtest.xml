<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE testset SYSTEM "/usr/local/share/rtest.dtd">

<testset>
    <fileversion>0.1</fileversion>
    <var name="COMMAND">rocalution-test --gtest_color=yes --gtest_output=xml</var>
    <var name="GTEST_FILTER">*</var>
    <test sets="psdb">
        <run name="checkin">{COMMAND}:output_psdb.xml --gtest_filter=*checkin*</run>
    </test>
    <test sets="osdb">
        <run name="nightly">{COMMAND}:output_osdb.xml --gtest_filter=*nightly*</run>
    </test>
    <test sets="custom">
        <run name="custom_filter">{COMMAND}:output_custom.xml --gtest_filter={GTEST_FILTER}</run>
    </test>
</testset>
